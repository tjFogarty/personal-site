{% set themeNames = ['bioshock', 'eleganza', 'ocean'] %}

{% set themes = {
  bioshock: {
    primary: '#197b8f',
    secondary: '#292b28'
  },
  eleganza: {
    primary: '#ac3b61',
    secondary: '#123c69'
  },
  ocean: {
    primary: '#116466',
    secondary: '#211e1c'
  }
} %}

{% set theme = random(themeNames) %}
style="--c-primary: {{ themes[theme].primary }}; --c-secondary: {{ themes[theme].secondary }};" data-theme-name="{{ theme }}"